/**
 * Implementation of hook_votingapi_insert
* @ingroup votingapi
 */
function votingpoints_votingapi_insert($votes) {
  $vote = (object)$votes[0];  
  $user = user_load($vote->uid);
  $node = node_load($vote->content_id);
  //dsm($vote);
  rules_invoke_event('votingpoints_event',$user, $node, $vote);  
}